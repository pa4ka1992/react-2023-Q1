{
  "name": "react-components",
  "version": "1.0.0",
  "description": "React + TypeScript + Vite + Github Actions",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "template",
    "ghactions",
    "vitest"
  ],
  "author": "nikita kisly",
  "license": "MIT",
  "homepage": "",
  "type": "module",
  "scripts": {
    "dev": "nodemon server/server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build --outDir dist/client",
    "build:server": " tsc && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "lint": "eslint --ext js,ts,tsx src",
    "test:vi": "vitest run --coverage",
    "test:cy": "npx start-server-and-test dev http-get://localhost:3000 cy:run-e2e && npx nyc report && npx nyc report --reporter=text-summary",
    "vitest:watch": "vitest",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:e2e": "start-server-and-test dev http-get://localhost:3000 cy:open-e2e"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "msw": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt-cli": "^17.0.3",
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "c8": "^7.12.0",
    "compression": "^1.7.4",
    "cross-fetch": "^3.1.5",
    "cypress": "^12.10.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "happy-dom": "^9.3.2",
    "husky": "^8.0.1",
    "jsdom": "^21.1.1",
    "lint-staged": "^11.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "sass": "^1.58.3",
    "serve-static": "^1.15.0",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",

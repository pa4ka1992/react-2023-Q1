{
  "all": true,
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "**/*.test.ts*",
    "**/*index.ts",
    "**/*_types.ts",
    "**/*_constants.ts",
    "**/types/**",
    "**/constants/**",
    "**/setupTests.ts",
    "**/withProviders.tsx",
    "**/entry-server.tsx"
  ]
}
